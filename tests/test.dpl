struct Point {
    x: i32;
    y: i32;
}

struct Circle {
    center: Point;
    radius: i32;
}

fn main() -> i32 {
    let p: Point = new Point {
        x = 10;
        y = 5;
    };

    let c: Circle = new Circle {
        center = p;
        radius = 10;
    };

    let c2: Circle = new Circle {
        center = new Point {
            x = 100;
            y = 50;
        };
        radius = 1;
    };

    printf("%i\n", c2.center.x);

    0
}
